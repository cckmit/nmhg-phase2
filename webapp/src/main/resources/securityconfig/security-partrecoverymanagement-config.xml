			</permissions>
		</resource>
		
		<resource type="ui" value="partsRecoveryAwaitingShipment" operation="OR">
			<permissions>partsRecovery:partsRecoveryAwaitingShipment:update,partsRecovery:partsRecoveryAwaitingShipment:view
			</permissions>
		</resource>
		
		<resource type="ui" value="partsRecoveryAwaitingShipmentToWarehouse" operation="OR">
