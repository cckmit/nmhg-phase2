<!DOCTYPE validators PUBLIC 
    "-//Apache Struts//XWork Validator 1.0.2//EN"   
    "http://struts.apache.org/dtds/xwork-validator-1.0.2.dtd">
<validators>
	<field name="reportSearchBean.fromDate">
		<field-validator type="required">
			<message key="error.downloadMgt.fromDateRequired"/>
		</field-validator>
		<field-validator type="date">
			<message key="error.downloadMgt.fromDateRequired"/>
		</field-validator>
	</field>

	<field name="reportSearchBean.toDate">
		<field-validator type="required">
			<message key="error.downloadMgt.toDateRequired"/>
		</field-validator>
		<field-validator type="date">
			<message key="error.downloadMgt.toDateRequired"/>
		</field-validator>
	</field>
</validators>