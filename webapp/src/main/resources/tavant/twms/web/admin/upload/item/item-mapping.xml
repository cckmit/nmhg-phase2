<!DOCTYPE root>
<excel>
  <sheet sheetName="Sheet1" type="tavant.twms.web.admin.dto.ItemDTO" startRow="1">
    <block>
      <column-mapping column="0" expression="itemNumber" />
      <column-mapping column="1" expression="description" />
      <column-mapping column="2" expression="productType" convertorName="itemGroupConvertor"/>
      <column-mapping column="3" expression="make" />
      <column-mapping column="4" expression="model" />
      <column-mapping column="5" expression="isSerialized" />
      <column-mapping column="6" expression="hasUsageMeter" />
    </block>
    <block-break column="0"/>
    <sheet-end/>
  </sheet>
</excel>